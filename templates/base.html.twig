<!DOCTYPE html>
<html dir="ltr" lang="{% block lang %}{% endblock %}">
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<meta name="description" content="{% block desc %}{% endblock %}"/>
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<meta name="robots" content="index, follow">
		<link
		rel="icon" href="">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css" integrity="sha512-1cK78a1o+ht2JcaW6g8OXYwqpev9+6GqOkz9xmBN9iUUhIndKtxwILGWYOSibOKjLsEdjyjZvYDq/cZwNeak0w==" crossorigin="anonymous" referrerpolicy="no-referrer" />
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" integrity="sha512-DTOQO9RWCH3ppGqcWaEA1BIZOC6xxalwEsw9c2QQeAIftl+Vegovlnee1c9QX4TctnWMn13TZye+giMm8e2LwA==" crossorigin="anonymous" referrerpolicy="no-referrer"/>
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/tarteaucitronjs/1.16.1/css/tarteaucitron.min.css" integrity="sha512-0YJD41h4qgomGCSVIcrv9X90zcHBmcoJVnEnIeeEjn97wGTvz6u4C2MtckrX6xaTsro5ihIPoJ82NCsPuDB5/A==" crossorigin="anonymous" referrerpolicy="no-referrer"/>
		{% endblock %}
	</head>
	<body {{ stimulus_controller('symfony/ux-swup/swup', { containers: ['#main', '#nav'], animateHistoryBrowsing: true, animationSelector: '[class*="transition-"]', cache: true, theme: 'fade', debug: true } ) }} data-damping='{% block damping %}{% endblock %}' data-scrollimg='{% block scrollimg %}{% endblock %}'>

		{{ include('webpages/blocks/header.html.twig') }}

		<div id="content">
			<div id="website">
				<main id="main"> 
					{% block body %}{% endblock %}
				</main>
				{% include "webpages/blocks/footer.html.twig" %}
			</div>
		</div>

			{% block javascripts %}
				<script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/js/all.min.js" integrity="sha512-GWzVrcGlo0TxTRvz9ttioyYJ+Wwk9Ck0G81D+eO63BaqHaJ3YZX9wuqjwgfcV/MrB2PhaVX9DkYVhbFpStnqpQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
                <script src="https://cdnjs.cloudflare.com/ajax/libs/tarteaucitronjs/1.16.1/tarteaucitron.min.js" integrity="sha512-KbSPuBkI4t3MyKmdytoAO4aNMdoEVVxUpGSxvVRWvK/f3HlMKu8Pns0NFm2AY1/09cVQAgUMIBC65iT3A1A6Fw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
				{{ encore_entry_script_tags('modernizr') }}
				{{ encore_entry_script_tags('app') }}
				{{ encore_entry_script_tags('tarteaucitronjs') }}
			{% endblock %}
		</body>
	</html>
